RailsApp::Application.routes.draw do
 

  devise_for :brokers

resources :agreements 
resources :vendors do
	 resources :items
end
resources :brokers

resources :customers do
member do
	get 'agreements'
end
resources :departments, :only => 'index'
end
resources :pos
resources :departments


  get 'new' => 'home#new'
  get 'characters_in_movie' => 'home#characters_in_movie'
  
  root :to => 'home#index'
end
