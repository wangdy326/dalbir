
= content_for :page_specific_scripts do
  = javascript_include_tag "pos"
  :javascript
    var $path_base = "/agreements/" + #{@agreement._id} + "/";
    var grid_data = #{@pos.to_json};
    jQuery(function($) {
      var grid_selector = "#pos-table";
      var pager_selector = "#pos-pager";

      jQuery(grid_selector).jqGrid({
        data: grid_data
      });
    });
%div.page-content
  %div.page-header
    %h1 Listing Itmes
  .row
    .col-xs-12
      %table#pos-table
        #pos-pager
